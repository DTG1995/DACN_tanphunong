@using DACN_TanPhuNong.Models
@model List<DACN_TanPhuNong.Models.tb_DoiTac>
@{
    ViewBag.Title = GlobalRes.ListPartners;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="text-center" style="text-transform:uppercase;">@GlobalRes.ListPartners</h2>
<div class="row">
    <div class="col-sm-12">

        @{
            foreach (var item in Model)
            {
                <div style="float:right;padding:20px 20px 20px 20px;">
                    <img src="@item.HinhAnh" />
                </div>
            }
        }
    </div>


</div>
