@using DACN_TanPhuNong.Controllers
@model IEnumerable<DACN_TanPhuNong.Models.tb_VanPhong>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Thông tin đại lý</h2>


@foreach (var item in Model) {
    <section class="post">
        <p><b>Tên đại lý: </b>@item.TenVP</p>
        <p><b>Địa chỉ: </b>@item.DiaChi</p>
        <p><b>Số điện thoại: </b>@item.TenVP</p>
        <p><b>Email: </b>@item.TenVP</p>
        <a href="@Url.RouteUrl("DaiLyDetails",new{id=item.MaVP, title=UrlEncode.ToFriendlyUrl(item.TenVP)})" title="@item.TenVP">Xem giới thiệu</a>
    </section>
    
}

